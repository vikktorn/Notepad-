<NotepadPlus>
    <UserLang name="Kotlin_Darcula_NEW" ext="kt kts" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="no" allowFoldOfComments="yes" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
            <Prefix Keywords1="no" Keywords2="no" Keywords3="no" Keywords4="no" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no" />
        </Settings>

        <KeywordLists>
            <!-- comments -->
            <Keywords name="Comments">00// 01/* 02*/</Keywords>

            <!-- numbers -->
            <Keywords name="Numbers, prefix1">0x 0b</Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1">f F L</Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>

            <!-- operators -->
            <Keywords name="Operators1">
                ++ -- .. ? ?. !! ?: :: -&gt;
                + - * / % = == === != !==
                &lt; &lt;= &gt;= &gt;
                &amp;&amp; || += -= *= /= %=
            </Keywords>
            <Keywords name="Operators2"></Keywords>

            <!-- folding on braces -->
            <Keywords name="Folders in code1, open">{</Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close">}</Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>

            <!-- core Kotlin keywords (orange) -->
            <Keywords name="Keywords1">
                package import typealias
class interface object enum annotation
data sealed open abstract final inner
companion override
constructor init
this super
fun val var get set field
by where
if else when
for while do
try catch finally throw
return break continue
in !in is !is as as?
true false null
public private protected internal
lateinit const
inline noinline crossinline
vararg
out
suspend
external
tailrec infix operator
expect actual
            </Keywords>

            <!-- annotations (pink) -->
            <Keywords name="Keywords2">
                Composable Preview
                Deprecated Suppress SinceKotlin
                JvmStatic JvmOverloads JvmField JvmName
                Throws Volatile Synchronized Strictfp Transient
                Target Retention MustBeDocumented Repeatable
                file Param ArrayVararg
                Immutable Stable ReadOnlyComposable
                Test Before After BeforeClass AfterClass Ignore
            </Keywords>

            <!-- common types (purple) -->
            <Keywords name="Keywords3">
               Unit Nothing Any
               Boolean BooleanArray
               Byte Short Int Long
               UByte UShort UInt ULong
               Float Double
               Char CharArray
               String StringBuilder 

               List MutableList
               Set MutableSet
               Map MutableMap
               Collection MutableCollection
               Iterable Sequence
               Array IntArray LongArray FloatArray DoubleArray 
               Pair Triple
               Result

              Throwable Exception RuntimeException
              IllegalArgumentException IllegalStateException
              IndexOutOfBoundsException NullPointerException
              NoSuchElementException UnsupportedOperationException

Regex MatchResult
File InputStream OutputStream
ByteArrayInputStream ByteArrayOutputStream

CoroutineScope Deferred Job
Flow StateFlow SharedFlow
Channel Mutex

Color Brush Modifier
Text Image Icon Icons
Scaffold Surface Card
Button OutlinedButton IconButton
FloatingActionButton
TopAppBar BottomAppBar
NavigationBar NavigationRail
Snackbar AlertDialog

Column Row Box
LazyColumn LazyRow
LazyVerticalGrid LazyHorizontalGrid

MaterialTheme Typography Shapes
remember rememberSaveable
mutableStateOf derivedStateOf
State MutableState SnapshotStateList SnapshotStateMap

Context Activity Fragment View ViewGroup
Intent Bundle Drawable Bitmap
LiveData MutableLiveData ViewModel
            </Keywords>

            <Keywords name="Keywords4"></Keywords>
            <Keywords name="Keywords5"></Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>

            <!-- real string delimiters:
                 00 = "   01 = "   02 = \
                 03 = ${ 04 =      05 = }
            -->
            <Keywords name="Delimiters">00&quot; 01&quot; 02\\ 03${ 04 05}</Keywords>
        </KeywordLists>

        <Styles>
            <!-- base text -->
            <WordsStyle name="DEFAULT"       fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />

            <!-- comments -->
            <WordsStyle name="COMMENTS"      fgColor="808080" bgColor="2B2B2B" fontName="Consolas" fontStyle="2" nesting="0" />
            <WordsStyle name="LINE COMMENTS" fgColor="808080" bgColor="2B2B2B" fontName="Consolas" fontStyle="2" nesting="0" />

            <!-- numbers -->
            <WordsStyle name="NUMBERS"       fgColor="6897BB" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />

            <!-- keywords -->
            <WordsStyle name="KEYWORDS1"     fgColor="CC7832" bgColor="2B2B2B" fontName="Consolas" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS2"     fgColor="FF79C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS3"     fgColor="9876AA" bgColor="2B2B2B" fontName="Consolas" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS4"     fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS5"     fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS6"     fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS7"     fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS8"     fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />

            <!-- operators -->
            <WordsStyle name="OPERATORS"     fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />

            <!-- folding -->
            <WordsStyle name="FOLDER IN CODE1"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />

            <!-- strings & ${} parts -->
            <WordsStyle name="DELIMITERS1"   fgColor="6A8759" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS2"   fgColor="6A8759" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS3"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS4"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS5"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS6"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS7"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS8"   fgColor="A9B7C6" bgColor="2B2B2B" fontName="Consolas" fontStyle="0" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
